80 NEXT Z
90 FOR Z=4 TO 1 STEP -1
100 FOR Y=3 TO 1 STEP -1
110 FOR X=5 TO 1 STEP -1
120 PRINT "A(";X;",";Y;",";Z;")=";A(X,Y,Z)
130 NEXT X
140 NEXT Y
150 NEXT Z
160 END
