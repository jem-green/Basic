10 DIM A(5,3,4)
11 LET A=5,B=3,C=4
40 FOR Z=1 TO C
30 FOR Y=1 TO B
20 FOR X=1 TO A
50 LET A(X,Y,Z)=X+(Y-1)*A+(Z-1)*A*B
60 NEXT X
